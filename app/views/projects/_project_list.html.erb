<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Status</th>
      <th>Type</th>
      <th>Project</th>
      <th>Created At</th>
    </tr>
  </thead>
  <tbody>
    <% projects.each do |project| %>
      <tr>
        <td><%= project.name %></td>
        <td><%= project.description %></td>
        <td><%= project.created_at.strftime("%Y-%m-%d") %></td>
      </tr>
    <% end %>
  </tbody>
</table>
